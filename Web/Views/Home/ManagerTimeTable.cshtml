@{
    ViewBag.Title = "RoomManager";
}

<div class="row">
    <form action="" method="post">
        <div class="row">
            <input type="text" placeholder="Room" class="center-block input-lg" name="room" />
        </div>

        <div class="row">
            <input type="submit" class="center-block btn" value="Get Time" />
        </div>
    </form>

    <p class="lead">@Html.Raw(@ViewBag.Message) </p>
</div>